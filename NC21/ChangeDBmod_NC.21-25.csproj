  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MultiCAD.NET" Version="21.0.5699.3427" />
    <PackageReference Include="nanoCAD.Platform.NET" Version="21.0.5699.3427" />
  </ItemGroup>

</Project>
