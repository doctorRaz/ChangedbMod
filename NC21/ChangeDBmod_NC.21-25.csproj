	<UseWPF>true</UseWPF>-->
	<UseWindowsForms>true</UseWindowsForms>
	  <Description>Переключалка баз Multicad для nanoCAD 21-25</Description>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
